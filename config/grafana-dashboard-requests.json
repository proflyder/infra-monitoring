          "expr": "sum by (method) (count_over_time({job=\"currency-bot\"} |~ `(GET|POST|PUT|DELETE|PATCH) - /(?:api|telegram)` | regexp `(?P<method>GET|POST|PUT|DELETE|PATCH)` [$__interval]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "legendFormat": "{{method}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
